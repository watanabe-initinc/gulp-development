{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "no-regex-spaces": 0,
    "no-multi-spaces": 0,
    "dot-notation": 0,
    "strict": 0
  }
}
